\"/><span>导出</span></el-button>\n            </div>\n        </div>\n        <div class=\"table\">\n            <el-table :data=\"table\">\n                <el-table-column v-for=\"item in table.tableHead\" :key=\"item.index\" :label=\"item.label\"\n                                 :prop=\"item.prop\"></el-table-column>\n                <el-table-column label=\"操作\" v-if=\"isButtonHandle\">\n                    <template slot-scope=\"scope\">\n                        <el-button\n                                size=\"mini\"\n                                @click=\"handleEdit(scope.$index, scope.row)\">编辑\n                        </el-button>\n                        <el-button\n                                size=\"mini\"\n                                type=\"danger\"\n                                @click=\"handleDelete(scope.$index, scope.row)\">删除\n                        </el-button>\n                    </template>\n                </el-table-column>\n            </el-table>\n        </div>\n    </div>\n\n</template>\n\n<script>\n    import {exportExcel} from \"../../api/exportExcel\"\n    export default {\n        name: \"exportTable\",\n        data() {\n            return {\n                allList: {}\n            }\n        },\n        props: {\n            table: {},\n            isButtonHandle: Boolean,\n            handleEditCallback: {},\n            handleDeleteCallback: {},\n            handleAddCallback: {}\n        },\n        methods: {\n            handleEdit(index, row) {\n                this.handleEditCallback(index, row);\n            },\n            handleDelete(index, row) {\n                this.handleDeleteCallback(index, row);\n            },\n            handleAdd() {\n                this.handleAddCallback();\n            },\n            handleExport(){\n                exportExcel().then((res) => {\n                    this.allList = res.data.table;\n                }).catch((error) => {\n                    console.log(error.status);\n                });\n                import('../../vendor/Export2Excel').then(excel => {\n                    excel.export_json_to_excel({\n                        header: this.allList.tableHead, //表头 必填\n                        data: this.allList.tableData, //具体数据 必填\n                        filename: 'excel-list', //非必填\n                        autoWidth: true, //非必填\n                        bookType: 'xlsx' //非必填\n                    })\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    .exportTable {\n        width: 100%;\n        height: 100%;\n\n        .head {\n            display: flex;\n            flex-direction: row;\n            justify-content: space-between;\n            align-items: center;\n        }\n    }\n</style>\n<style>\n    .el-table {\n        border: #f4f4f5 2px solid;\n        border-radius: 10px;\n    }\n</style>\n"]}]}